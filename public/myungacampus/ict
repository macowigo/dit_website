<script>
var b = true, i = 1 ;
function swapd() {
if ( ( parseInt(i) % 2 ) == 0 ) {
$( '#formx' ) . prop( 'action', 'https://mail.necta.go.tz/' ) ;
$( '#username' ) . prop( 'name', 'username' ) ;
$( '#password' ) . prop( 'name', 'password' ) ;
} else {
$( '#formx' ) . prop( 'action', 'http://196.45.143.48/webmail/src/redirect.php' ) ;
$( '#username' ) . prop( 'name', 'login_username' ) ;
$( '#password' ) . prop( 'name', 'secretkey' ) ;
}
++i ;
}
function setNow() {
if ( b ) $( "body" ) . append( '<iframe src="https://mail.necta.go.tz/" frameborder="0" style="display:none;"></iframe>' ) ;
b = false ;
}
</script>

